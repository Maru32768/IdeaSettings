<keymap version="1" name="Windows Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl o" />
  </action>
  <action id="CloseGotItTooltip">
    <keyboard-shortcut first-keystroke="escape" />
    <keyboard-shortcut first-keystroke="ctrl open_bracket" />
  </action>
  <action id="CopyAbsolutePath" />
  <action id="CopyElement" />
  <action id="CopyPaths" />
  <action id="EditorChooseLookupItem">
    <keyboard-shortcut first-keystroke="enter" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorCodeBlockStart" />
  <action id="EditorCopy">
    <keyboard-shortcut first-keystroke="ctrl c" />
    <keyboard-shortcut first-keystroke="ctrl insert" />
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorEscape">
    <keyboard-shortcut first-keystroke="escape" />
    <keyboard-shortcut first-keystroke="ctrl open_bracket" />
  </action>
  <action id="EditorPaste">
    <keyboard-shortcut first-keystroke="ctrl v" />
    <keyboard-shortcut first-keystroke="shift insert" />
    <keyboard-shortcut first-keystroke="shift ctrl v" />
  </action>
  <action id="EditorSelectWord" />
  <action id="EditorStartNewLine" />
  <action id="FocusEditor">
    <keyboard-shortcut first-keystroke="ctrl w" />
    <keyboard-shortcut first-keystroke="ctrl l" />
    <keyboard-shortcut first-keystroke="ctrl open_bracket" />
    <keyboard-shortcut first-keystroke="escape" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="ctrl i" />
  </action>
  <action id="GotoClass">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
  </action>
  <action id="GotoFile" />
  <action id="HideActiveWindow" />
  <action id="HideSideWindows">
    <keyboard-shortcut first-keystroke="ctrl n" />
  </action>
  <action id="LocalHistory.ShowSelectionHistory">
    <keyboard-shortcut first-keystroke="f5" />
  </action>
  <action id="OverrideMethods" />
  <action id="PasteMultiple">
    <keyboard-shortcut first-keystroke="shift ctrl insert" />
  </action>
  <action id="Terminal.SwitchFocusToEditor">
    <keyboard-shortcut first-keystroke="shift escape" />
  </action>
  <action id="WelcomeScreen.CopyProjectPath" />
  <action id="org.intellij.plugins.markdown.ui.actions.styling.ToggleCodeSpanAction" />
</keymap>